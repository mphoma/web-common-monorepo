    "babel-preset-minify": "^0.5.0",
    "del-cli": "^2.0.0",
    "nyc": "^14.1.1",
    "pmm": "^2.0.0",
    "pre-commit": "^1.2.2"
  },
  "browserslist": [
    ">0.2%",
    "not dead"
  ]
